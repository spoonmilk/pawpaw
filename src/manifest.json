{
  "manifest_version": 3,
  "name": "pawpaw",
  "description": "A privacy-conscious chrome extension with zing",
  "options_page": "options.html",
  "background": { "service_worker": "background.bundle.js" },
  "permissions": ["activeTab", "storage", "activeTab", "tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*terms-of-service*",
        "*://*/*terms-and-conditions*",
        "*://*/*terms-of-use*",
        "*://*/*youtube.com/t*",
        "*://*/*privacy-policy*",
        "*://*/*cookie-policy*",
        "*://*/*cookie-notice*",
        "*://*/*privacy-notice*",
        "*://*/*tos*",
        "*://*/*eula*",
        "*://*/*sla*",
        "*://*/*policies*",
        "*://*/*license-agreement*",
        "*://*/*license*",
        "*://*/*license-notice*",
        "*://*/*user-agreement*",
        "*://*/*legal*",
        "*://*/*agreement*"
      ],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
    }
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
